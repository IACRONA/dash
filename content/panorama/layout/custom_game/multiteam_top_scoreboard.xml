<root>
	<styles>
		<include src="s2r://panorama/styles/dotastyles.vcss_c" />
		<include src="s2r://panorama/styles/custom_game/multiteam_top_scoreboard.vcss_c" />
		<include src="s2r://panorama/styles/custom_game/shared_scoreboard_styles.vcss_c" />
	</styles>
	<scripts>
		<include src="s2r://panorama/scripts/custom_game/shared_scoreboard_updater.vjs_c" />
		<include src="s2r://panorama/scripts/custom_game/multiteam_top_scoreboard.vjs_c" />
	</scripts>
	<Panel hittest="false" class="CustomHudRoot">

		<Panel hittest="false" id="TopBarScoreboard">
			<Panel id="KillToWin" class="KillToWin">
                <MoviePanel class="TimeVideo" src="file://{resources}/videos/red_ball.webm" repeat="true" autoplay="onload" />
                <Label class="KillToWinLabel" id="KillToWinLabel" text="" />
                <Label class="FlagCounter" id="FlagCounter" text="00:00" />
			</Panel>
			<Label class="TimeUntil" id="TimeUntil" text="#time_until_1" />
			<Label class="TimeUntil" id="TimeUntil2" text="#time_until_2" />
			<Panel hittest="false" id="ScoreboardContainer">
				<Panel hittest="false" id="LeftInjectXMLFile" />
				<Panel hittest="false" id="MultiteamScoreboard" />
				<Panel hittest="false" id="RightInjectXMLFile" />
			</Panel>
		</Panel>
	</Panel>
</root>
